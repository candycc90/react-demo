{
  "name": "react-demo",
  "version": "1.0.0",
  "description": "demo for React-Babel-Webpack project",
  "main": "app/main.js",
  "scripts": {
    "lint": "eslint --cache --ignore-path .gitignore --ext .jsx,.js --format=node_modules/eslint-formatter-pretty .",
    "precommit": "npm run lint",
    "test": "npm run lint",
    "build": "webpack --config ./webpack.production.config.js --progress",
    "start": "webpack-dev-server --progress"
  },
  "dependencies": {
    "react": "15.x",
    "react-dom": "15.x",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "react-redux": "^5.0.4"
  },
  "devDependencies": {
    "babel-core": "6.x",
    "babel-eslint": "7.x",
    "babel-loader": "6.x",
    "babel-preset-es2015": "6.x",
    "babel-preset-react": "6.x",
    "css-loader": "~0.26.1",
    "eslint": "latest",
    "eslint-config-airbnb": "latest",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-plugin-compat": "^1.0.0",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsx-a11y": "3.x",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "latest",
    "html-webpack-plugin": "latest",
    "node-sass": "latest",
    "open-browser-webpack-plugin": "0.0.3",
    "sass-loader": "latest",
    "style-loader": "~0.13.1",
    "webpack": "^2.4.1",
    "webpack-dev-server": "latest"
  },
  "keywords": [
    "react",
    "web app"
  ],
  "author": "candycc",
